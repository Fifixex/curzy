{
  "name": "@uniteds/curzy",
  "version": "1.0.3",
  "description": "Library to create a quick and easy discord bot",
  "module": "src/index.ts",
  "type": "module",
  "author": "Unitedss",
  "license": "MIT",
  "keywords": ["discord-bot", "generator", "curzy"],
  "repository": {
    "type": "git",
    "url": "https://github.com/unitedss/curzy.git"
  },
  "homepage": "https://github.com/unitedss/curzy",
  "bugs": "https://github.com/unitedss/curzy/issues",
  "files": ["dist/**/*"],
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "release": "semantic-release",
    "lint": "biome lint --write src/**/*.ts",
    "fmt": "biome format --write .",
    "build": "tsup"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/bun": "latest",
    "@types/node": "^22.0.2",
    "dotenv": "^16.4.5",
    "install": "^0.13.0",
    "lefthook": "^1.7.11",
    "semantic-release": "^24.0.0",
    "tsup": "^8.2.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
